{
    "manifest_version": 2,
    "name": "HistoryX",
    "description": "A Chrome extension to enhance your browsing history experience.",
    "version": "2.3.0",
    "icons": {
        "16": "assets/img/icon16.png",
        "48": "assets/img/icon48.png",
        "128": "assets/img/icon128.png"
    },
    "chrome_url_overrides" : {
      "history": "index.html"
    },
    "permissions": [
        "history",
        "chrome://favicon/"
    ]
}